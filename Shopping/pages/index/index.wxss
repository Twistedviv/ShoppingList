.item-container{
  width: 750rpx;
  height: 81px;
  z-index: 10;
  margin:3rpx 0 3rpx 0;
  background:no-repeat url("data:image/png;base64,");
  background-size: 100% 100%;
}

.item{
  width: 750rpx;
  height: 75px;
  z-index: 20;
  display: flex;
  flex-direction: row;
  border-top: solid #e5e5e5 1px;      
}

.name-container{
  width: 550rpx;
  position: relative;
  
}


.item-name-background{
  width: 470rpx;
  height: 85%;
  position: absolute;
  left: 30rpx;
  top: 15rpx;
  background:no-repeat url("data:image/png;base64,");
  background-size: 100% 100%;
}

.item-name{
  position: absolute;
  left: 50rpx;
  top:40rpx;

}

.price-container{
  width: 200rpx;
  position: relative;
  
}


.item-price-background{
  width: 200rpx;
  height: 55%;
  position: absolute;
  right: -5rpx;
  top: 13rpx;
  background:no-repeat url("data:image/png;base64,") ;
  background-size: 100% 100%;
  font-size: 13px;
  text-align: right;
  line-height: 41rpx;
}

.item-price{
  position: absolute;
  right: 5rpx;
  top:22rpx

}


.add-item{
  background:#ffffff no-repeat 50% url("data:image/png;base64,");
}

.in{
  margin-bottom: 3px;
  border: solid 1px #000000;
}